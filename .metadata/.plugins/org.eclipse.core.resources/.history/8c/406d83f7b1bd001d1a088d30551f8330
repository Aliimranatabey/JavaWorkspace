
public class Main {

	public static void main(String[] args) {
		
		String a="Merhaba";
		System.out.println(a+" Hoşgeldiniz...");
		
		String b="Java";
		String c="Programlama";
		String d="Dili";
		
		String toplam=b+c+d;
		System.out.println("Toplam : "+toplam);
		
		String k="Merhaba";
		int z=35;
		String h=k+z;
		System.out.println("h : "+h);
		
		String p="Merhaba";
		double o=3.52;
		byte y=10;
		p=p+o+y;
		System.out.println("p : "+p);
		
		char u='?';
		String r="Merhaba nasılsın"+u;
		System.out.println("r : "+r);

	}

}
